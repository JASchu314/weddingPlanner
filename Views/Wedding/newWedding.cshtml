    <input class ="form-control" asp-for="wedderOne"/><br>
    <label asp-for="wedderTwo"></label>
    <input class ="form-control" asp-for="wedderTwo"/><br>
    <label asp-for="date"></label>
    <input class ="form-control"asp-for="date"/><br>
    <label asp-for="wedAddress"></label>
    <input class ="form-control" asp-for="wedAddress"/><br>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
</fieldset>